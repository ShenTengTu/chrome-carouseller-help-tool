{
  "name": "chrome-carouseller-help-tool",
  "version": "0.0.1",
  "description": "Help seller making various operations conveniently on Carousell.",
  "scripts": {
    "manifest-d": "set NODE_ENV=development&& node src/manifest.json.js",
    "manifest-p": "set NODE_ENV=production&& node src/manifest.json.js",
    "webpack-d": "set NODE_ENV=development&& webpack",
    "webpack-p": "set NODE_ENV=production&& webpack",
    "b-d": "run-s manifest-d webpack-d",
    "b-p": "run-s manifest-p webpack-p"
  },
  "keywords": [
    "carousell",
    "chrome extension"
  ],
  "author": "Shen-Teng Tu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShenTengTu/chrome-carouseller-help-tool.git"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "jquery": "^3.2.1",
    "jquery-sendkeys": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "less-loader": "^4.0.5",
    "npm-run-all": "^4.1.1",
    "source-map-loader": "^0.2.1",
    "webpack": "^3.5.5"
  },
  "peerDependencies": {
    "less": "^2.3.1"
  }
}
